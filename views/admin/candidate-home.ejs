<%- contentFor('content') %>

<!-- Main Page Content -->
<div class="container"><br><br>

    <!-- Folder Tabs -->
    <div class="row">
        <div class="col-lg-12">
            <ul id="tabs" class="nav nav-tabs tabtop" style="border-bottom: 10px solid #EFEFEF;">
                <li <% if(tab == 'profile'){ %> class="active" <% } %> >
                    <a href="/profile">
                    <i class="fa fa-user"></i> Profile</a>
                </li>

                <li <% if(tab == 'messages'){ %> class="active" <% } %>>
                    <a href="/profile/messages">
                    <i class="fa fa-inbox"></i> Messages</a>
                </li>
            </ul>

            <div id="tabsContent">
                <div id="profile">
                    <% if(tab == 'profile'){ %>
                        <%- include candidate/profile-tab  %>
                    <% } %>
                    <% if(tab == 'messages'){ %>
                        <%- include messages  %>
                    <% } %>
                </div>
            </div>

        </div>
    </div>
</div?
